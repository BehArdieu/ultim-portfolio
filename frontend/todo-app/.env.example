# Variables d'environnement pour l'application Todo
# Copier ce fichier vers .env.local pour le développement

# Environment
NODE_ENV=development

# Application
REACT_APP_NAME=Todo App
REACT_APP_VERSION=1.0.0

# API (si backend ajouté plus tard)
REACT_APP_API_URL=http://localhost:3001/api

# Analytics (optionnel)
REACT_APP_GOOGLE_ANALYTICS_ID=

# Features flags (optionnel)
REACT_APP_ENABLE_DARK_MODE=true
REACT_APP_ENABLE_EXPORT=true